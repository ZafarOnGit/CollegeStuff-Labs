dist = float(input("Enter the distance in KM: "));
if dist >= 40 or dist < 0:
    print("Invalid distance entered")
    
else:
    if dist < 5:
        price = "FREE"
    elif dist >= 5:
        price = 15;
    elif dist >= 20:
        price = 25;

    print(f"The delivery fee is {price}")

